class CalculatorLogic:
    def calculate(self, num1: float, operator: str, num2: float) -> float:
        try:
            num1 = float(num1)
            num2 = float(num2)
            if operator == '+':
                return num1 + num2
            elif operator == '-':
                return num1 - num2
            elif operator == '*':
